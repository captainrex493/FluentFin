<?xml version="1.0" encoding="utf-8" ?>
<Page
    x:Class="FluentFin.Plugins.Playback_Reporting.Views.PlaybackReportPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="using:FluentFin.Plugins.Playback_Reporting.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:sp="using:ScottPlot.WinUI"
    xmlns:wtv="using:WinUI.TableView"
    mc:Ignorable="d">

    <Grid Margin="36">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>

        <sp:WinUIPlot x:Name="Plot" Height="1000" />

    </Grid>
</Page>
